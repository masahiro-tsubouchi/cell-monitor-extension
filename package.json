{
  "name": "jupyter-extension-system",
  "version": "1.0.0",
  "description": "JupyterLab Cell Monitor Extension System",
  "scripts": {
    "dev": "docker compose up --build",
    "prod": "docker compose -f docker-compose.prod.yml up --build",
    "stop": "docker compose down",
    "clean": "docker compose down -v && ./scripts/docker-cleanup.sh",
    "logs": "docker compose logs -f",
    "status": "docker compose ps",
    "rebuild": "docker compose down && docker compose build --no-cache && docker compose up"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": ["jupyter", "extension", "monitoring", "education"],
  "license": "MIT"
}
